{
    "name": "clicker",
    "productName": "Clicker",
    "version": "1.0.0",
    "description": "Application which can run scripts via UI",
    "homepage": "https://dev.azure.com/dieslnikolas/Clicker",
    "author": {
        "name": "Nikolas Diesl",
        "email": "dieslnikolas@gmail.com"
    },
    "keywords": [
        "angular",
        "angular 12",
        "electron",
        "nodejs",
        "typescript",
        "spectron",
        "eslint",
        "sass",
        "windows",
        "mac",
        "linux"
    ],
    "scripts": {
        "installdeps": "concurrently \"npm run api:installdeps\" \"npm run shell:installdeps\" \"npm run client:installdeps\"",
        "build": "concurrently \"npm run api:build\" \"npm run shell:build\" \"npm run client:build\"",
        "start": "concurrently \"npm run api:start\" \"npm run client:start\" \"npm run shell:start\"",
        "development": "concurrently \"npm run api:development\" \"npm run client:development\" \"npm run shell:development\"",
        "api:installdeps": "cd Clicker.API && npm run installdeps",
        "api:build": "cd Clicker.API && npm run build",
        "api:start": "cd Clicker.API && npm run start",
        "api:development": "cd Clicker.API && npm run development",
        "client:installdeps": "cd Clicker.Client && npm run installdeps",
        "client:build": "cd Clicker.Client && npm run build",
        "client:start": "cd Clicker.Client && npm start",
        "client:development": "cd Clicker.Client && npm run development",
        "shell:installdeps": "cd Clicker.Shell && npm run installdeps",
        "shell:build": "cd Clicker.Shell && npm run build",
        "shell:start": "cd Clicker.Shell && npm run start",
        "shell:development": "cd Clicker.Shell && npm run development",
        "other:package": "todo: INSTALACE BALICKU",
        "other:postinstall": "UPRAVY POST INSTALL - PUSTI SE VZDY",
        "other:version": "cd Clicker.Client && version"
    }
}
