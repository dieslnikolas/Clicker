{
    "name": "clicker",
    "productName": "Clicker shell",
    "version": "1.0.0",
    "main": "main.js",
    "private": true,
    "scripts": {
        "installdeps": "npm ci && cp package.json ../../dist/shell && cd ../../dist/shell && npm install --only=production",
        "build": "tsc -p tsconfig.json",
        "start": "cd ../../dist/shell && npx electron ./main.js --serve"
    },
    "dependencies": {
        "@electron/remote": "1.2.0",
        "electron": "^18.2.3",
        "electron-reload": "^2.0.0-alpha.1",
        "fs-extra": "^10.0.1",
        "log4js": "^6.4.4"
    },
    "devDependencies": {
        "electron-builder": "^23.0.3",
        "typescript": "^4.6.4"
    }
}
