{
  "name": "clicker",
  "version": "",
  "main": "dist/server/main.js",
  "scripts": {
    "angular-run": "ng serve",
    "angular-build": "ng build --base-href ./",
    "electron-run": "electron .",
    "electron-build": "tsc --p electron",
    "electron": "concurrently -k \"npm run angular-run\" \"wait-on http://localhost:4200 && npm run electron-build && npm run electron-run\"",
    "release-windows": "npm run angular-build && npm run electron-build && npm run electron-run",
    "release-macos": "npm run angular-build && npm run electron-build && npm run electron-run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.1",
    "@angular/cdk": "^11.2.3",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/forms": "~11.1.1",
    "@angular/material": "^11.2.3",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "bootstrap": "^4.6.0",
    "electron-log": "^4.3.1",
    "jquery": "^3.5.1",
    "keytar": "^7.4.0",
    "ngx-electron": "^2.2.0",
    "node-ssh": "^11.1.1",
    "popper": "^1.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.2",
    "@angular/cli": "~11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@types/electron": "^1.6.10",
    "@types/node": "^12.11.1",
    "concurrently": "^5.3.0",
    "electron": "^10.1.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.1"
  }
}
